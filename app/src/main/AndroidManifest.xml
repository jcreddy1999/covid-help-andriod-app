<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bosscoder.covidhelp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CovidHelp">
        <activity android:name=".AboutUs.AboutUsActivity"></activity>
        <activity android:name=".CovidTracker.DetailActivity" />
        <activity android:name=".CovidTracker.TrackerMainActivity" />
        <activity android:name=".CovidTracker.AffectedCountries" />
        <activity android:name=".CovidVacciine.ResaultShowActivity" />
        <activity android:name=".CovidVacciine.VaccineActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>